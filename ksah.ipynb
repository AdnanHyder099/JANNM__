    "def matrixAddition(A,B):\n",
    "    C = np.zeros(A.shape)\n",
    "    for i in range(A.shape[0]):\n",
    "        for j in range(A.shape[1]):\n",
    "            C[i,j] = A[i,j] + B[i,j]\n",
    "    return(C)"
